#include <iostream>

using namespace std;

int main()
{
    //menampilkan menghitung nilai faktorial

    int i, n, hasil_faktorial;

    cout << " Masukkan Angka : ";
    cin >> n;
    i=n;
    hasil_faktorial=1;
    while (i>1) {
        hasil_faktorial=hasil_faktorial*i;
        i--;
    }
    cout <<"Hasil Faktorial : "<< hasil_faktorial;
    return 0;
}
